{
  "name": "myvendor/laravel-modules",
  "description": "A Laravel 12 modular system package for creating and managing application modules.",
  "version": "0.2.0",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "modules", "modular", "package"],
  "authors": [
    {"name": "Your Name", "email": "you@example.com"}
  ],
  "require": {
    "php": ">=8.3",
    "illuminate/support": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Modules\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Modules\\ModulesServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "format": "vendor/bin/pint"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0",
    "laravel/pint": "^1.16"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
